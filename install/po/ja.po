msgstr "Base64形式のデコードに失敗: %(reason)s"

msgid "must be an integer"
msgstr "整数である必要があります"

#, python-format
msgid "must be at least %(minvalue)d"
msgstr "少なくとも %(minvalue)d である必要があります"

